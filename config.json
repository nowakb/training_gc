{
  "auth": {
    "enabled": true,
    "rules": {
      "^/$": {
        "loginRequired": true,
        "redirectUrl": "/login"
      },

      "^/index.*": {
        "loginRequired": true,
        "redirectUrl": "/login"
      }
    }
  },

  "logging": {
    "categories": {
      "rest": "warn"
    }
  }
}
