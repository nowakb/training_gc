{
  "socket.io": {
    "enabled": true,
    "host": "localhost"
  },
  "auth": {
    "enabled": true,
    "rules": {
      "^/$": {
        "loginRequired": true,
        "redirectUrl": "/login"
      },

      "^/index.*": {
        "loginRequired": true,
        "redirectUrl": "/login"
      }
    }
  }
}
